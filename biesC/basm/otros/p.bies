let isPalindromeRecur = (lst) =>
    let {
        const check = (i, j) =>
            if (i >= j) then true
            else if (lst[i] != lst[j]) then false
            else check(i + 1, j - 1)
    } in check(0, len(lst) - 1)

// Ejemplo:
let a = [1, 8, 3, 2, 1]
print("isPalindromeRec ["+a+"] ?")
print(isPalindromeRecur(a)) // Retorna true
